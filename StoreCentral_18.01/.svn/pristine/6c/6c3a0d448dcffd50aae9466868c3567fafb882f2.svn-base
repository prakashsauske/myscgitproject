package au.com.woolworths.portal.model;

import org.codehaus.jackson.annotate.JsonCreator;
import org.codehaus.jackson.annotate.JsonIgnoreProperties;
import org.codehaus.jackson.annotate.JsonProperty;

@JsonIgnoreProperties(ignoreUnknown = true)
public class GoodReceiptDetailforPO {
	

	@JsonProperty("gr_no")
	private String grNo;

	@JsonProperty("received_date")
	private String receivedDate;
	
	@JsonProperty("gi_value")
	private String giValue;

	@JsonProperty("delv_dock_no")
	private String delvDockNo;

	@JsonProperty("gr_doc_year")
	private String grDocYear;

	@JsonProperty("tot_car_recvd")
	private String totCarReceived;

	@JsonProperty("iv_site")
	private String ivSite;

	@JsonProperty("iv_order_no")
	private String ivOrderNo;
	
	
	
	//Newly added JSON creator

	
	@JsonProperty("grNo")
	private String grNo_m;

	@JsonProperty("receivedDate")
	private String receivedDate_m;
	
	@JsonProperty("giValue")
	private String giValue_m;

	@JsonProperty("delvDockNo")
	private String delvDockNo_m;

	@JsonProperty("grDocYear")
	private String grDocYear_m;

	@JsonProperty("totCarReceived")
	private String totCarReceived_m;

	@JsonProperty("ivSite")
	private String ivSite_m;

	@JsonProperty("ivOrderNo")
	private String ivOrderNo_m;
	

	/**
	 * @param grNo
	 * @param receivedDate
	 * @param giValue
	 * @param delvDockNo
	 * @param grDocYear
	 * @param totCarReceived
	 * @param ivSite
	 * @param ivOrderNo
	 */
	@JsonCreator
	public GoodReceiptDetailforPO(@JsonProperty("gr_no") String grNo,@JsonProperty("received_date") String receivedDate,
			@JsonProperty("gi_value") String giValue,@JsonProperty("delv_dock_no") String delvDockNo,@JsonProperty("gr_doc_year") String grDocYear,
			@JsonProperty("tot_car_recvd") String totCarReceived,@JsonProperty("iv_site") String ivSite,@JsonProperty("iv_order_no") String ivOrderNo) {
		
		this.grNo = grNo;
		this.receivedDate = receivedDate;
		this.giValue = giValue;
		this.delvDockNo = delvDockNo;
		this.grDocYear = grDocYear;
		this.totCarReceived = totCarReceived;
		this.ivSite = ivSite;
		this.ivOrderNo = ivOrderNo;
		
		this.grNo_m = grNo;
		this.receivedDate_m = receivedDate;
		this.giValue_m = giValue;
		this.delvDockNo_m = delvDockNo;
		this.grDocYear_m = grDocYear;
		this.totCarReceived_m = totCarReceived;
		this.ivSite_m = ivSite;
		this.ivOrderNo_m  = ivOrderNo;
	}
	/**
	 * @return the grNo
	 */
	public String getGrNo() {
		return grNo;
	}
	/**
	 * @param grNo the grNo to set
	 */
	public void setGrNo(String grNo) {
		this.grNo = grNo;
	}

	/**
	 * @return the receivedDate
	 */
	public String getReceivedDate() {
		return receivedDate;
	}

	/**
	 * @param receivedDate the receivedDate to set
	 */
	public void setReceivedDate(String receivedDate) {
		this.receivedDate = receivedDate;
	}

	/**
	 * @return the giValue
	 */
	public String getGiValue() {
		return giValue;
	}

	/**
	 * @param giValue the giValue to set
	 */
	public void setGiValue(String giValue) {
		this.giValue = giValue;
	}

	/**
	 * @return the delvDockNo
	 */
	public String getDelvDockNo() {
		return delvDockNo;
	}

	/**
	 * @param delvDockNo the delvDockNo to set
	 */
	public void setDelvDockNo(String delvDockNo) {
		this.delvDockNo = delvDockNo;
	}

	/**
	 * @return the grDocYear
	 */
	public String getGrDocYear() {
		return grDocYear;
	}

	/**
	 * @param grDocYear the grDocYear to set
	 */
	public void setGrDocYear(String grDocYear) {
		this.grDocYear = grDocYear;
	}

	/**
	 * @return the totCarReceived
	 */
	public String getTotCarReceived() {
		return totCarReceived;
	}

	/**
	 * @param totCarReceived the totCarReceived to set
	 */
	public void setTotCarReceived(String totCarReceived) {
		this.totCarReceived = totCarReceived;
	}

	/**
	 * @return the ivSite
	 */
	public String getIvSite() {
		return ivSite;
	}

	/**
	 * @param ivSite the ivSite to set
	 */
	public void setIvSite(String ivSite) {
		this.ivSite = ivSite;
	}

	/**
	 * @return the ivOrderNo
	 */
	public String getIvOrderNo() {
		return ivOrderNo;
	}

	/**
	 * @param ivOrderNo the ivOrderNo to set
	 */
	public void setIvOrderNo(String ivOrderNo) {
		this.ivOrderNo = ivOrderNo;
	}



}

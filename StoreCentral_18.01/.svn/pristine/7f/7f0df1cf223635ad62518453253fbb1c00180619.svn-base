							<label for="UOM" class="">Unit of Measure (UOM):</label>
								<span id="radioHistoryContentFilter">
								</span>		
						</span>
					</h4>
					
				</div>
				<!-- End of table title -->

			</div>
			<!-- End of table info -->
			
		</div>
		<!-- End of pop up data -->

			<div class="ContentTableWrapper contentHead">
			
			</div>
	
			<div class="ContentTableWrapper contentBody">
				
			</div>
	
		<!-- End of content table wrapper -->

		<div class="popupActionsWrapper">
			<span class="popupActions"> <label
				class="actionBtn saleHistorySaveBtn">Save</label> <label
				class="actionBtn closeBtn">Close</label>
			</span>
		</div>
		<!-- End of popup actions-->



	</div>
	<!-- End of popupContent -->
</div>
<!-- End of Promotion Sales history popup -->






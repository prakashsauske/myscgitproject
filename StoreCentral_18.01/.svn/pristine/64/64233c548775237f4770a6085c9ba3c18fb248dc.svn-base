package au.com.woolworths.portal.model;

import org.codehaus.jackson.annotate.JsonIgnoreProperties;
import org.codehaus.jackson.annotate.JsonProperty;

@JsonIgnoreProperties(ignoreUnknown = true)
public class ArticleProductNotes {

	@JsonProperty("article")
	private String article;
	@JsonProperty("product_note_1")
	private String productNote1;
	@JsonProperty("product_note_2")
	private String productNote2;
	@JsonProperty("product_note_3")
	private String productNote3;
	@JsonProperty("tasting_note_1")
	private String tastingNote1;
	@JsonProperty("tasting_note_2")
	private String tastingNote2;
	@JsonProperty("tasting_note_3")
	private String tastingNote3;
	@JsonProperty("tasting_note_4")
	private String tastingNote4;
	@JsonProperty("tasting_note_5")
	private String tastingNote5;

	public String getArticle() {
		return article;
	}

	public void setArticle(String article) {
		this.article = article;
	}

	public String getProductNote1() {
		return productNote1;
	}

	public void setProductNote1(String productNote1) {
		this.productNote1 = productNote1;
	}

	public String getProductNote2() {
		return productNote2;
	}

	public void setProductNote2(String productNote2) {
		this.productNote2 = productNote2;
	}

	public String getTastingNote1() {
		return tastingNote1;
	}

	public void setTastingNote1(String tastingNote1) {
		this.tastingNote1 = tastingNote1;
	}

	public String getTastingNote2() {
		return tastingNote2;
	}

	public void setTastingNote2(String tastingNote2) {
		this.tastingNote2 = tastingNote2;
	}

	public String getTastingNote3() {
		return tastingNote3;
	}

	public void setTastingNote3(String tastingNote3) {
		this.tastingNote3 = tastingNote3;
	}

	public String getTastingNote4() {
		return tastingNote4;
	}

	public void setTastingNote4(String tastingNote4) {
		this.tastingNote4 = tastingNote4;
	}

	public String getTastingNote5() {
		return tastingNote5;
	}

	public void setTastingNote5(String tastingNote5) {
		this.tastingNote5 = tastingNote5;
	}

	/**
	 * @return the productNote3
	 */
	public String getProductNote3() {
		return productNote3;
	}

	/**

<div id="dialog-modal" title="User Role Management">
	<div class="popupContent">

		<div class="popupData">
			<h4 class="alertText">User Preferences has been saved
				successfully.</h4>

			<div class="popupActionsWrapper">
				<span class="popupActions"> <label class="actionBtn okBtn"
					onclick="$('#dialog-modal').dialog('close');">Ok</label>
				</span>
			</div>
			<!-- End of popup actions-->
		</div>
		<!-- End of pop up data -->

	</div>
	<!-- End of popupContent -->
</div>

<div id="dialog-replicate" title="Replicate Settings">
	<div class="popupContent wizardContent">
		<div class="popupData">

			<h4 class="alertText">
				Replicate current notification settings to other Sales Org & Roles
			</h4>
			<form method="POST" action="" id="wizard">
				<div class="formWrapper step-1-form" title="Select sales org and apply">
					<h2 class="wizardTitle">Select sales org to apply settings</h2>
					<div class="ContentTableWrapper">
							<table cellspacing="0" class="ContentTable">
								<thead>
								<tr>
									<th class=" centerValue" width="10px"> </th>
									<th class="lastColumn">Sales Org </th>
								</tr>
								</thead>
								<tbody id="replicate-salesOrg">
								
								</tbody>
							</table>
					</div>
					<!-- End of main tab 4 -->


				</div>
				<!-- End of form wrapper step 1 -->

				<div class="formWrapper step-2-form" title="Select roles">
					<h2 class="wizardTitle">Select roles to apply settings</h2>
					<div class="ContentTableWrapper " style="max-height: 375px!important; overflow-y: scroll!important;">
						<table cellspacing="0" class="ContentTable">
							<thead>
								<tr>
									<th class=" centerValue" width="10px"><input
										type="checkbox" name="rpl_roleAll"></th>
									<th class="lastColumn">Roles</th>
								</tr>
							</thead>
							<tbody id="relicate-roles">
								<tr>
									<td class=" centerValue"><input type="checkbox" name="sel"></td>
									<td class="lastColumn">Department Manager</td>
								</tr>

							</tbody>
						</table>
					</div>
					<!-- End of content table wrapper -->
				</div>
				<!-- End of form wrapper step 2 -->
			</form>
		</div>
		<!-- End of pop up data -->
		<div class="popupActionsWrapper hideBlock">
			<span class="popupInfo"> <label class="mandatory">mandatory</label>
			</span> <span class="popupActions"> <label class="actionBtn">Apply</label>
				<label class="secondaryActionBtn">Cancel</label>
			</span>
		</div>
		<!-- End of popup actions-->

	</div>
	<!-- End of popupContent -->
</div>

<!-- All Edit functions -->
<div id="dialog-editFunctions" title="Add Departments">
	<div class="popupContent">
		<div class="popupData">
				<h4 class="alertText hideBlock">Select what departments you want to add</h4>

					<div class="ContentTableWrapper">
						<div class="tableActionsWrapper" id="tableAddAction" style="height: 255px;">

							<form method="POST" action="" id="articleForm">
								<div class="formWrapper">

									<div class="parameter clearfix">
							<div class="hierarchyWrapper hierarchySingle clearfix" style="background: none!important;border: none!important;">

								<!-- Department -->
								<div class="hierarchyContent lastContent" id="deptDiv">

									<div class="hierarchyList" >
										<ul>
											<li class="alreadyAddedRow"><input type="checkbox" name="departmentList" value="deptValue1" id="deptValue1"><label for="deptValue1" class="labelText">General Merchandise</label></li>
											<li class="alreadyAddedRow"><input type="checkbox" name="departmentList" value="deptValue2" id="deptValue2"><label for="deptValue2" class="labelText">Perishables</label></li>
											<li class="alreadyAddedRow"><input type="checkbox" name="departmentList" value="deptValue3" id="deptValue3"><label for="deptValue3" class="labelText">Groceries</label></li>
											<li class="alreadyAddedRow"><input type="checkbox" name="departmentList" value="deptValue4" id="deptValue4"><label for="deptValue4" class="labelText">Non Trading</label></li>
											<li class="alreadyAddedRow"><input type="checkbox" name="departmentList" value="deptValue5" id="deptValue5"><label for="deptValue5" class="labelText">Meat</label></li>
											<li class="alreadyAddedRow"><input type="checkbox" name="departmentList" value="deptValue6" id="deptValue6"><label for="deptValue6" class="labelText">Liquor</label></li>
											<li class="alreadyAddedRow"><input type="checkbox" name="departmentList" value="deptValue7" id="deptValue7"><label for="deptValue7" class="labelText">Personal Care</label></li>
											<li class="alreadyAddedRow"><input type="checkbox" name="departmentList" value="deptValue8" id="deptValue8"><label for="deptValue8" class="labelText">Health</label></li>
											<li class="alreadyAddedRow"><input type="checkbox" name="departmentList" value="deptValue9" id="deptValue9"><label for="deptValue9" class="labelText">Baked Items</label></li>
											<li class="alreadyAddedRow"><input type="checkbox" name="departmentList" value="deptValue10" id="deptValue10"><label for="deptValue10" class="labelText">Coupons</label></li>
										</ul>
									</div> <!-- End of hierarchy Title -->


								</div> <!-- End of hierarchy Content -->


							</div> <!-- end of hierarchy Wrapper -->


						</div>




								</div> <!-- End of content table wrapper -->
							</form>



						</div> <!-- End of table Actions Wrapper -->

					</div> <!-- End of ContentTableWrapper -->


			</div>
		<!-- End of pop up data -->

		<div class="popupActionsWrapper">
			<span class="popupActions"> <label class="actionBtn"
				id="createOrderButton"><a ><label
						class="thumbUp addstore-popup-save-btn">Save</label></a></label> <label class="secondaryActionBtn addstore-popup-cancel-btn"><a
					>Cancel</a></label>
			</span>
		</div>
		<!-- End of popup actions-->
	</div>
	<!-- End of popupContent -->
</div>
<!-- End of popup -->
<div id="dialog-modal-alertBox" title="Notification Settings">
	<div class="popupContent">

		<div class="popupData">


			<h4 class="alertText" id="alertBox"></h4>



			<div class="popupActionsWrapper">
				<span class="popupActions"> <label class="actionBtn"
					onmousedown="$( '#dialog-modal-alertBox' ).dialog( 'close' );"
					id="okBtn">OK</label>
					<label class="actionBtn hideBlock yesBtn"
					id="okBtn">Yes</label>
					<label class="actionBtn hideBlock noBtn"
					id="okBtn">No</label>
				</span>
			</div>
			<!-- End of popup actions-->



		</div>
	</div>
	<!-- End of popupContent -->
</div>
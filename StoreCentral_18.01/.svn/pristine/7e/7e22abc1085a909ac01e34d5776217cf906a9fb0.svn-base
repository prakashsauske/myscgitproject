package au.com.woolworths.portal.model;

import java.util.List;

import org.codehaus.jackson.annotate.JsonIgnoreProperties;
import org.codehaus.jackson.annotate.JsonProperty;

@JsonIgnoreProperties(ignoreUnknown = true)
public class SiteHierarchyDetailsResponseHelper {

	@JsonProperty("results")
	private List<PromoArticleReview> PromoArticleReview;

	public List<PromoArticleReview> getSiteHierarchyDetailsList() {
		return PromoArticleReview;
	}

	public void setSiteHierarchyDetailsList(
			List<PromoArticleReview> siteHierarchyDetailsList) {
		PromoArticleReview = siteHierarchyDetailsList;
	}

}

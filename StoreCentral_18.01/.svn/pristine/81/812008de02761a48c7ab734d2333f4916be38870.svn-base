@media print {
	/* make a 10-inch high block that sits on top of the page content */
	.page-break {
		page-break-before: always;
		display: block;
		position: absolute;
		width: 100%;
	}
	.pageBreak {
		page-break-before: always;
	}

	/* stick the contents of .page-break to the bottom of the 10 inch block */
	.page-break .copyright {
		position: absolute;
		bottom: 0px;
		/* width:100% */
	}
}


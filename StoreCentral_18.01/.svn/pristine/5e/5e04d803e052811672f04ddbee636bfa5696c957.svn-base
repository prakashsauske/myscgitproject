package au.com.woolworths.portal.param;

import org.codehaus.jackson.annotate.JsonIgnoreProperties;
import org.codehaus.jackson.annotate.JsonProperty;

@JsonIgnoreProperties(ignoreUnknown = true)
public class StockFillReportParam {

	@JsonProperty("iv_site_no")
	private String iv_site_no;

	@JsonProperty("iv_sales_org")
	private String iv_sales_org;

	@JsonProperty("iv_session")
	private String iv_session;

	@JsonProperty("iv_barcode")
	private String iv_barcode;

	@JsonProperty("iv_barcode_flag")
	private String iv_barcode_flag;

	@JsonProperty("iv_department")
	private String iv_department="ALL";

	@JsonProperty("iv_article")
	private String iv_article;

	@JsonProperty("iv_lto_barcode")
	private String iv_lto_barcode;

	@JsonProperty("iv_action")
	private String iv_action = "Y";
	
	@JsonProperty("iv_refresh_flg")
	private String iv_refresh_flg;

	public String getIv_site_no() {
		return iv_site_no;
	}

	public void setIv_site_no(String iv_site_no) {
		this.iv_site_no = iv_site_no;
	}

	public String getIv_sales_org() {
		return iv_sales_org;
	}

	public void setIv_sales_org(String iv_sales_org) {
		this.iv_sales_org = iv_sales_org;
	}

	public String getIv_session() {
		return iv_session;
	}

	public void setIv_session(String iv_session) {
		this.iv_session = iv_session;
	}

	public String getIv_barcode() {
		return iv_barcode;
	}

	public void setIv_barcode(String iv_barcode) {
		this.iv_barcode = iv_barcode;
	}

	public String getIv_barcode_flag() {
		return iv_barcode_flag;
	}

	public void setIv_barcode_flag(String iv_barcode_flag) {
		this.iv_barcode_flag = iv_barcode_flag;
	}

	public String getIv_department() {
		return iv_department;
	}

	public void setIv_department(String iv_department) {
		this.iv_department = iv_department;
	}

	public String getIv_article() {
		return iv_article;
	}

	public void setIv_article(String iv_article) {
		this.iv_article = iv_article;
	}

	public String getIv_lto_barcode() {
		return iv_lto_barcode;
	}

	public void setIv_lto_barcode(String iv_lto_barcode) {
		this.iv_lto_barcode = iv_lto_barcode;
	}

	public String getIv_action() {
		return iv_action;
	}

	public void setIv_action(String iv_action) {
		this.iv_action = iv_action;
	}

	/**
	 * @return the iv_refresh_flg
	 */
	public String getIv_refresh_flg() {
		return iv_refresh_flg;
	}

	/**
	 * @param iv_refresh_flg the iv_refresh_flg to set
	 */
	public void setIv_refresh_flg(String iv_refresh_flg) {
		this.iv_refresh_flg = iv_refresh_flg;
	}

}

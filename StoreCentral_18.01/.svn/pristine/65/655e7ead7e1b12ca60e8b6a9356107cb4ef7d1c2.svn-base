		return active_to;
	}

	/**
	 * @return the salesOrg
	 */
	public String getSalesOrg() {
		return salesOrg;
	}

	/**
	 * @return the roles
	 */
	public ArrayList<String> getRoles() {
		return roles;
	}

	/**
	 * @return the siteOrRegion
	 */
	public ArrayList<String> getSiteOrRegion() {
		return siteOrRegion;
	}

	/**
	 * @return the isAck
	 */
	public String getIsAck() {
		return isAck;
	}

	/**
	 * @return the isReaded
	 */
	public String getIsReaded() {
		return isReaded;
	}

	/**
	 * @param msg the msg to set

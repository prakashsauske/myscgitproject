package au.com.woolworths.portal.model;

import java.util.ArrayList;

import org.codehaus.jackson.annotate.JsonProperty;

public class BroadcastMessage {

	@JsonProperty("msg")
	private String msg;
	
	@JsonProperty("typ")
	private String typ;

	@JsonProperty("messageId")
	private String messageId;

	@JsonProperty("message")
	private String message;

	@JsonProperty("active_from")
	private String active_from;

	@JsonProperty("active_to")
	private String active_to;

	@JsonProperty("salesOrg")
	private String salesOrg;

	@JsonProperty("roles")
	private ArrayList<String> roles;

	@JsonProperty("siteOrRegin")
	private ArrayList<String> siteOrRegion;

	@JsonProperty("isAck")
	private String isAck;

	@JsonProperty("isReaded")
	private String isReaded;

	@JsonProperty("isAckReq")
	private String isAckReq;
	
	@JsonProperty("isLimitReached")
	private String isLimitReached;
	
	/**
	 * @return the isLimitReached
	 */
	public String getIsLimitReached() {
		return isLimitReached;
	}

	/**
	 * @param isLimitReached the isLimitReached to set
	 */
	public void setIsLimitReached(String isLimitReached) {
		this.isLimitReached = isLimitReached;
	}

	/**
	 * @return the isAckReq
	 */
	public String getIsAckReq() {
		return isAckReq;
	}

	/**
	 * @param isAckReq the isAckReq to set
	 */
	public void setIsAckReq(String isAckReq) {
		this.isAckReq = isAckReq;
	}

	/**
	 * @return the maxAckReq
	 */
	public String getMaxAckReq() {
		return maxAckReq;
	}

	/**
	 * @param maxAckReq the maxAckReq to set
	 */
	public void setMaxAckReq(String maxAckReq) {
		this.maxAckReq = maxAckReq;
	}

	@JsonProperty("maxAckReq")
	private String maxAckReq;
	
	/**
	 * @return the msg
	 */
	public String getMsg() {
		return msg;
	}

	/**
	 * @return the typ
	 */
	public String getTyp() {
		return typ;
	}

	/**
	 * @return the messageId
	 */
	public String getMessageId() {
		return messageId;
	}

	/**
	 * @return the message
	 */
	public String getMessage() {
		return message;
	}

	/**
	 * @return the active_from
	 */
	public String getActive_from() {
		return active_from;
	}

	/**
	 * @return the active_to
	 */

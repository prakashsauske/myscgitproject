package au.com.woolworths.portal.model;

public class UserInfoModel {

	private String user_id;

	private String site_no;
	
	private String sales_org;

	private String role_id;

	private String active_start_date;
	
	private String active_end_date;
	
	public UserInfoModel(String user_id, String site_no,String sales_org, String role_id,
			String active_start_date, String active_end_date,
			String isPrimaryRole, String isStoreRoleExist,
			String isStoreRoleActive, String isAdminRoleExist,
			String isAdminRoleActive) {
		super();
		this.user_id = user_id;
		this.site_no = site_no;
		this.role_id = role_id;
		this.active_start_date = active_start_date;
		this.active_end_date = active_end_date;
		this.sales_org=sales_org;	
		
		if(isPrimaryRole!=null && isPrimaryRole.equalsIgnoreCase("Y"))
			this.isPrimaryRole =true;
		else
			this.isPrimaryRole =false;
		
		if(isStoreRoleExist!=null && isStoreRoleExist.equalsIgnoreCase("Y"))
			this.isStoreRoleExist =true;
		else
			this.isStoreRoleExist =false;
		
		if(isStoreRoleActive!=null && isStoreRoleActive.equalsIgnoreCase("Y"))
			this.isStoreRoleActive =true;
		else
			this.isStoreRoleActive =false;
		
		if(isAdminRoleExist!=null && isAdminRoleExist.equalsIgnoreCase("Y"))
			this.isAdminRoleExist =true;
		else
			this.isAdminRoleExist =false;
		
		if(isAdminRoleActive!=null && isAdminRoleActive.equalsIgnoreCase("Y"))
			this.isAdminRoleActive =true;
		else
			this.isAdminRoleActive =false;
		
	}

	/**
	 * @return the sales_org
	 */
	public String getSales_org() {
		return sales_org;
	}

	/**
	 * @param sales_org the sales_org to set
	 */
	public void setSales_org(String sales_org) {
		this.sales_org = sales_org;
	}

	public UserInfoModel() {
	
	}

	private boolean isPrimaryRole;
	
	private boolean isStoreRoleExist;
	
	private boolean isStoreRoleActive;
	
	private boolean isAdminRoleExist;
	
	private boolean isAdminRoleActive;

	/**
	 * @return the user_id
	 */
	public String getUser_id() {
		return user_id;
	}

	/**
	 * @return the site_no
	 */
	public String getSite_no() {
		return site_no;
	}

	/**
	 * @return the role_id
	 */
	public String getRole_id() {
		return role_id;
	}

	/**
	 * @return the active_start_date
	 */
	public String getActive_start_date() {
		return active_start_date;
	}

	/**
	 * @return the active_end_date
	 */
	public String getActive_end_date() {
		return active_end_date;
	}

	/**
	 * @return the isPrimaryRole
	 */
	public boolean isPrimaryRole() {
		return isPrimaryRole;
	}

	/**
	 * @return the isStoreRoleExist
	 */
	public boolean isStoreRoleExist() {
		return isStoreRoleExist;
	}

	/**
	 * @return the isStoreRoleActive
	 */
	public boolean isStoreRoleActive() {
		return isStoreRoleActive;
	}

	/**
	 * @return the isAdminRoleExist
	 */
	public boolean isAdminRoleExist() {
		return isAdminRoleExist;
	}

	/**
	 * @return the isAdminRoleActive
	 */
	public boolean isAdminRoleActive() {
		return isAdminRoleActive;
	}

	/**
	 * @param user_id the user_id to set
	 */
	public void setUser_id(String user_id) {
		this.user_id = user_id;
	}

	/**
	 * @param site_no the site_no to set
	 */
	public void setSite_no(String site_no) {
		this.site_no = site_no;
	}

	/**
	 * @param role_id the role_id to set
	 */
	public void setRole_id(String role_id) {
		this.role_id = role_id;
	}

	/**
	 * @param active_start_date the active_start_date to set
	 */
	public void setActive_start_date(String active_start_date) {
		this.active_start_date = active_start_date;
	}

	/**
	 * @param active_end_date the active_end_date to set
	 */
	public void setActive_end_date(String active_end_date) {
		this.active_end_date = active_end_date;
	}

	/**
	 * @param isPrimaryRole the isPrimaryRole to set
	 */
	public void setPrimaryRole(boolean isPrimaryRole) {
		this.isPrimaryRole = isPrimaryRole;
	}

	/**
	 * @param isStoreRoleExist the isStoreRoleExist to set
	 */
	public void setStoreRoleExist(boolean isStoreRoleExist) {
		this.isStoreRoleExist = isStoreRoleExist;
	}

	/**
	 * @param isStoreRoleActive the isStoreRoleActive to set
	 */
	public void setStoreRoleActive(boolean isStoreRoleActive) {
		this.isStoreRoleActive = isStoreRoleActive;
	}

	/**
	 * @param isAdminRoleExist the isAdminRoleExist to set
	 */
	public void setAdminRoleExist(boolean isAdminRoleExist) {
		this.isAdminRoleExist = isAdminRoleExist;
	}

	/**
	 * @param isAdminRoleActive the isAdminRoleActive to set
	 */
	public void setAdminRoleActive(boolean isAdminRoleActive) {
		this.isAdminRoleActive = isAdminRoleActive;
	}
	
	
	

}

package au.com.woolworths.portal.param;

import org.codehaus.jackson.annotate.JsonIgnoreProperties;
import org.codehaus.jackson.annotate.JsonProperty;

@JsonIgnoreProperties(ignoreUnknown = true)
public class StockTakeVarianceAddCriteriaParam {

	@JsonProperty("iv_field")
	private String iv_field;

	@JsonProperty("iv_criteria")
	private String iv_criteria;

	@JsonProperty("iv_value")
	private String iv_value;

	public String getIv_field() {
		return iv_field;
	}

	public void setIv_field(String iv_field) {
		this.iv_field = iv_field;
	}

	public String getIv_criteria() {
		return iv_criteria;
	}

	public void setIv_criteria(String iv_criteria) {
		this.iv_criteria = iv_criteria;
	}

	public String getIv_value() {
		return iv_value;
	}

	public void setIv_value(String iv_value) {
		this.iv_value = iv_value;
	}

}

package au.com.woolworths.portal.model;

import java.util.List;

import org.codehaus.jackson.annotate.JsonProperty;

public class LinkedArticleResponseHelper {

	@JsonProperty("results")
	private List<LinkedArticleData> linkedArticleDataList;

	/**
	 * @return the linkedArticleDataList
	 */
	public List<LinkedArticleData> getLinkedArticleDataList() {
		return linkedArticleDataList;
	}

	/**
	 * @param linkedArticleDataList
	 *            the linkedArticleDataList to set
	 */
	public void setLinkedArticleDataList(
			List<LinkedArticleData> linkedArticleDataList) {
		this.linkedArticleDataList = linkedArticleDataList;
	}

}

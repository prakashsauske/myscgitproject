	margin-left: 3px;
}

label.negativeStatus {
	padding-left: 16px;
	padding-right: 7px;
	padding-bottom: 2px;
	margin-left: 3px;
	background: url(../images/iconDeleteCircle.png) left 2px no-repeat;
	/*img change*/
	color: #df0b0b;
}


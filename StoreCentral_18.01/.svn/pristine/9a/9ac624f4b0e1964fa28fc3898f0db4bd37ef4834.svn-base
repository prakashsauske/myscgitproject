	font-size: 1em; /* 12px */
}

/* styles modified for alignment - Haresh 1st July 2013 */
.userProfile .ui-menu {
	border: 0;
	z-index: 500;
	text-align: right;
	float: right;
}

.userProfile .ui-menu ul li {
	text-align: left;
}

.userProfile .ui-menu a,.userProfile .ui-menu a:hover {
	border: 0;
}

.userProfile .ui-menu .ui-menu-item a {
	display: inline;
}

.userProfile  .ui-menu .ui-menu-item { /*width:200px;	*/
	
}


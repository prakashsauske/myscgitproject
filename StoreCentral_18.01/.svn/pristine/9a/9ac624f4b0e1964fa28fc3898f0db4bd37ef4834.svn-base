}

/* Checked state */
.selectDropdown .dropdown li input:checked  ~ label {
	color: grey;
}

/* Active state */
.selectDropdown.active:after {
	color: 0 6px 6px 6px;
}

.selectDropdown.active .dropdown {
	opacity: 1;
}


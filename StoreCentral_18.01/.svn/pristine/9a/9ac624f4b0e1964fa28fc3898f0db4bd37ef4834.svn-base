.wizardContent .jw-widget.jw-hasmenu .jw-steps-wrap {
	height: auto;
	min-height: 20em;
	/*padding-bottom: 10px;*/
}

.jw-widget .jw-steps-wrap {
	overflow: visible;
	padding: 0;
}

.jw-widget .jw-content {
	padding: 0;
	border: 0;
}

.wizardContent .ui-widget-content {
	border: none;
}

.wizardContent .ui-widget-content .ui-menu .ui-menu-item {
	width: auto;
	display: inline-block;
	/*white-space: nowrap;*/
}

.wizardContent .jw-widget .jw-footer .jw-buttons .jw-button-next,.wizardContent .jw-widget .jw-footer .jw-buttons .jw-button-finish
	{
	float: none;
}

.wizardContent .jw-widget .jw-footer {
	padding: 0;
	text-align: right;
}

.wizardContent .ui-widget-header {
	border: none;
}

.wizardContent .ui-button-text-only .ui-button-text {
	padding: 0;
}

.wizardContent .jw-buttons .actionBtn {
	font-weight: bold;
}

.wizardContent .jw-widget .jw-steps-wrap {
	
}

.wizardContent  .jw-menu {
	padding: 0;
}

.wizardContent .ui-menu .ui-menu-item {
	border-radius: 3px;
	font-size: 1.08em;
	padding: 5px 4px;
	border: none;
}

.wizardContent .ui-menu .ui-menu-item a {
	color: #828282;
}

.wizardContent .ui-menu .ui-menu-item a:hover,.wizardContent .ui-menu .ui-menu-item a:focus,.wizardContent .ui-menu .ui-menu-item a:visited
	{
	text-decoration: none;
}

.wizardContent .ui-widget-content .ui-state-disabled {
	opacity: 1;
	margin: 0;
}

.wizardContent .ui-widget-content .ui-state-disabled a {
	color: #5c5c5c;
}

.wizardContent .formWrapper {
	padding: 10px 7px;
}

.wizardContent .formWrapper h2.wizardTitle {
	font-size: 1.15em;
	padding-bottom: 15px;
	padding-top: 5px;
}

.wizardContent .formWrapper .warningMessage h4 {
	font-size: 1.1em;
}

.wizardContent .formWrapper .parameter .selectOptions {
	display: inline;
	line-height: 25px;
	padding-left: 7px;
}

.wizardContent .formWrapper .parameter .selectOptions label {
	width: auto;
}

.popupWrapper .ui-dialog-content .wizardContent .popupActionsWrapper {
	padding: 7px 10px;
}

.ContentTableWrapper  .tableFooter {
	height: 30px;
}

.tableFooter .legend {
	float: left;
	padding-top: 5px;
}

.formWrapper .parameter .linkBtn label {
	font-size: 0.9em;
}

/* Style for scroll to top */
.scrollup {
	background: #494848;
	color: white;
	font-weight: bold;
	padding: 3px 8px;
	position: fixed;
	bottom: 45px;
	right: 0;
	display: none;
}
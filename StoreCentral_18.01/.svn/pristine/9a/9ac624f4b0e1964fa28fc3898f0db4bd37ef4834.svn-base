/* Style for multi select option menu */

/*CheckBox dropdown*/
.selectDropdown { /* Size and position */
	/* position: absolute;*/
	display: inline-block;
	width: 170px;
	margin: 0 auto;
	padding: 2px 2px 2px 5px;
	/* Styles */
	cursor: pointer;
	outline: none;
	background: #fff url(../images/iconSelectDropdown.gif) 97% no-repeat;
	border: 1px solid #D9D9D9;
	border-radius: 3px 3px 3px 3px;
	color: #000000;
	font-size: 1em;
	padding: 4px;
	vertical-align: middle;
}

.ui-dialog .selectDropdown,.popupWrapper .selectDropdown {
	position: relative;
}

.selectDropdown  .selectLabel {
	display: block;
	cursor: pointer;
}

.selectDropdown .dropdown { /* Size & position */
	position: absolute;
	/*top: 100%;*/
	margin-left: -5px;
	margin-top: 3px; /* border of wrapper */
	/* left: -1px;
    right: -1px;*/
	/* Styles */
	background: white;
	border: 1px solid silver;
	list-style: none;
	transition: all 0.3s ease-out;
	/* Hiding */
	opacity: 0;
	width: 178px;
}

.selectDropdown .dropdown li {
	position: relative; /* Enable absolute positioning for checkboxes */
}

.selectDropdown .dropdown li label {
	cursor: pointer;
	color: #222;
	display: block;
	padding: 5px; /* Same padding as the button */
}

.tableActionsWrapper .formWrapper .parameter .selectDropdown .dropdown li label.dropdownLabel
	{
	padding-right: 15px;
}

.selectDropdown .dropdown li label.actionBtn,.selectDropdown .dropdown li label.secondaryActionBtn
	{
	display: inline-block;
	width: auto;
}

.selectDropdown .dropdown li:last-of-type label {
	border: none;
}

.selectDropdown .dropdown li input /* Checkboxes */ {
	position: absolute;
	display: block;
	right: 3px;
	top: 50%;
	margin-top: -8px;
}

.selectDropdown .dropdown li input.checkbox {
	cursor: pointer;
}

.articleHead .selectDropdown {
	background: none;
	border: none;
	width: auto;
	margin: 0;
	padding: 0;
	vertical-align: top;
	/*position:inherit;*/
}


/* Style for buttons with icons */
label.nearbyBtn {
	background: url(../images/iconNearby.png) left 1px no-repeat;
	padding-left: 19px;
}

label.editBtn {
	background: url(../images/iconEdit.png) 1px 3px no-repeat;
	padding-left: 19px;
}

label.notepad {
	background: url(../images/iconPadTick.png) left 1px no-repeat;
	padding-left: 18px;
}

label.notepadCross {
	background: url(../images/iconPadCross.png) left 1px no-repeat;
	padding-left: 18px;
}

label.thumbUp {
	background: url(../images/iconThumbUp.png) left 1px no-repeat;
	padding-left: 18px;
}

label.print {
	background: url(../images/iconPrint.png) left 2px no-repeat;
	padding-left: 22px;
}

label.positiveStatus {
	padding-left: 20px;
	padding-right: 7px;
	padding-bottom: 2px;
	background: url(../images/iconTickCircle.png) left -1px no-repeat;
	/*img change*/

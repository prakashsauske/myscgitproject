html {
	background: #f2f2f2 url(../images/verticalBg.png) center top repeat-y;
	margin: 0;
	padding: 0;
	height: 100%;
}

body {
	/*background: url(../images/verticalBg.png) center top repeat-y;*/
	font-family: Arial, Helvetica, sans-serif;
	font-size: 75%;
	margin: 0;
	padding: 0;
	height: 100%;
	overflow-y: scroll;
}

p,div,h1,h2,h3,h4,ul,li,form,button {
	margin: 0;
	padding: 0;
}

a {
	text-decoration: none;
}

a:hover,a:visited {
	text-decoration: none;
}

input.required_qty {
	text-align: right;
	width: 60px;
	height: 16px;
	margin: -8px -6px -6px -5px;
}
label:focus {
	border: dotted 1px red;
}

ul li {
	list-style: none;
}

ul.orderList {
	padding-left: 15px;
	margin-top: 5px;
}

ul.orderList li {
	list-style: disc;
	margin-top: 2px;
}

h2 {
	font-size: 1.333em; /* 16px */
}

h3 {
	display: inline-block;
	font-size: 1.083em; /* 13px */
	font-weight: normal;
}

h4 {
	font-size: 1.167em; /* 14px */
	font-weight: normal;
}

.centreAlign {
	text-align: center;
}

.mainWrapper {
	width: 980px;
	margin: 0 auto;
	height: auto;
}

.headWrapper {
	background: #fff;
}

.headWrapper .header {
	height: 105px;
}

.headWrapper .header .logoWrapper {
	float: left;
}


}

.articleRangedValue {
	
}

.articleRangedLabel {
	
}

.actionBtn,.secondaryActionBtn,.tableActionsWrapper .formWrapper .parameter .selectDropdown label.actionBtn,.tableActionsWrapper .formWrapper .parameter .selectDropdown label.secondaryActionBtn

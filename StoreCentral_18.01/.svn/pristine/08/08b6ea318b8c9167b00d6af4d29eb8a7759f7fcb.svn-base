/*Notification CSS - Duraivel - 24Aug*/
.mainWrapper .broadcastWrapper { /* height: 30px; */
	background-color: yellow;
	/* width: 1004px; */
	padding: 0 auto;
	font-size: 1em;
}

.mainWrapper .broadcastWrapper .broadcastContent {
	max-width: 980px;
	margin: 0 auto;
	/* min-height: 30px; */
	padding: 10px 0px;
	color: #333;
	position: relative;
}

.mainWrapper .broadcastWrapper .broadcastContent .broadcastText {
	width: 90%;
	font-size: 1.1em;
	cursor: pointer;
}

.mainWrapper .broadcastWrapper .broadcastContent .broadcastAction {
	padding: 4px 10px;
	float: right;
	text-align: center;
	border-radius: 2px;
	top: 5px;
	position: absolute;
	right: 0px;
	cursor: pointer;
	font-size: 1em;
}

.mainWrapper .broadcastDetails {
	position: absolute;
	float: right;
	right: 30px;
	width: 350px;
	max-height: 500px;
	min-height: 100px;
	top: 100px;
	background-color: #fdfdfd;
	border-radius: 3px;
	border: 1px solid #d4d4d4;
	box-shadow: 0px 0px 4px 0px #d4d4d4;
	z-index: 1;
}

.mainWrapper .broadcastDetails span.arrow-up {
	width: 0;
	height: 0;
	border-left: 8px solid transparent;
	border-right: 8px solid transparent;
	border-bottom: 8px solid #f4f4f4;
	position: absolute;
	right: 20px;
	top: -7px;
}

.mainWrapper .broadcastDetails .broadcastItem {
	padding: 15px 10px;
	border-bottom: 1px solid #d4d4d4;
	position: relative;
	min-height: 35px;
}

.mainWrapper .broadcastDetails .broadcastItem,.mainWrapper .broadcastDetails .broadcastItem div,.mainWrapper .broadcastDetails .broadcastItem label
	{
	cursor: pointer;
}

.mainWrapper .broadcastDetails .broadcastItem>div {
	padding-left: 5px;
	background-size: 30px;
	background-position: left;
	min-height: 30px;
}

.mainWrapper .broadcastDetails .broadcastItem.viewed label,.mainWrapper .broadcastDetails .broadcastItem.viewed div
	{
	opacity: 0.8;
}

.mainWrapper .broadcastDetails .broadcastItem.lastItem {
	border-bottom: 0px;
}

.mainWrapper .broadcastDetails .broadcastItem label.title {
	display: block;
	font-size: 1.2em;
	margin-bottom: 5px;
	padding: 0px;
	font-weight: normal;
}


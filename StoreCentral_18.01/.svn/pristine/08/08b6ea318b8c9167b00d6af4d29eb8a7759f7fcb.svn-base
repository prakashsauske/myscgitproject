    margin: 0;
    padding: 0;
}
label.refresh {
	background: url(../images/icon-Refresh.png) left -7px no-repeat;
    padding-left: 23px;
    background-size: 27px;
}
a {
    cursor: pointer;
    text-decoration: none;
}
a:hover,
a:visited {
    text-decoration: none;
}
label.actionBtn a,
label.secondaryActionBtn a {
    color: #fff;
}
.tempPadding {
    padding-top: 0!important;
}
input[type="radio"] {

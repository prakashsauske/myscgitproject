html {
    /*background:#fff url(../images/verticalBg.png) center top repeat-y; */
    height: 100%;
    margin: 0;
    padding: 0;
}
body {
    font-family: Arial, Helvetica, sans-serif;
    font-size: 75%;
    height: 100%;
    margin: 0;
    overflow-y: scroll;
    padding: 0;
}
p,
div,
h1,
h2,
h3,
h4,
h5,

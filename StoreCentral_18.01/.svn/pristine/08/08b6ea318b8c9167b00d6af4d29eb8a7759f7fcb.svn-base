}
.actionStatusMessage {
    background: #fffdb9;
    /* color change*/
    /* Safari 4-5, Chrome 1-9 */
    
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#fffdb9), to(#faf278));
    /* color change*/
    /* Safari 5.1, Chrome 10+ */
    
    background: -webkit-linear-gradient(top, #fffdb9, #faf278);
    /* color change*/
    /* Firefox 3.6+ */
    
    background: -moz-linear-gradient(top, #fffdb9, #faf278);
    /* color change*/
    
    border: solid 1px #e8e36a;
    /* color change*/
    
    border-radius: 5px;
    margin: 0 35%;
    padding: 10px 15px;
    position: absolute;
    top: 18px;
    width: 30%;
}
.actionStatusMessage .messageText {
    color: #686868;
    float: left;
    width: 90%;
}
.actionStatusMessage .messageClose {
    float: right;
    text-align: right;
    width: 10%;
}
.actionStatusMessage .messageClose a {
    background: url(../images/iconCrossCircle.png) right top no-repeat;
    padding-bottom: 3px;
    padding-left: 18px;
}
.mainWrapper {

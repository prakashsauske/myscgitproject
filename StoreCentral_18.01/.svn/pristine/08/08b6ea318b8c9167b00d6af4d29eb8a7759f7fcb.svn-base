/* Style for buttons with icons */

label.nearbyBtn {
    background: url(../images/iconNearby.png) left 1px no-repeat;
    padding-left: 19px;
}
label.editBtn {
    background: url(../images/iconEdit.png) 1px 3px no-repeat;
    padding-left: 19px;
}
label.addItem:after {
    background: url("../images/iconArrowDown_white.png") no-repeat;
    content: "";
    float: right;
    height: 17px;
    margin: 0;
    -webkit-transform: rotate(90deg);
    width: 9px;
}
label.removeItem:before {
    background: url("../images/iconArrowDown_white.png") no-repeat;
    content: "";
    float: left;
    height: 16px;
    margin: 0;
    -webkit-transform: rotate(270deg);
    width: 8px;
}
label.upItem:after {
    background: url("../images/iconArrowDown_white.png") 0 4px no-repeat;
    content: "";
    float: right;
    height: 15px;
    margin: 0;
    margin-left: 5px;
    -webkit-transform: rotate(0deg);
    width: 10px;
}
label.downItem:after {
    background: url("../images/iconArrowDown_white.png") 0 4px no-repeat;
    content: "";
    float: right;
    height: 15px;
    margin: 0;
    margin-left: 5px;
    -webkit-transform: rotate(180deg);
    width: 10px;
}

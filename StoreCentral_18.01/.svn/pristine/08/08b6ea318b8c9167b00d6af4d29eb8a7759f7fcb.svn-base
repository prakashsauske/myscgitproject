.actionBtn.disabled,
.actionBtn.disabled:hover {
    background: #e0e0e0;
    /* Safari 4-5, Chrome 1-9 */
    
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#e0e0e0), to(#d1d1d1));
    /* Safari 5.1, Chrome 10+ */
    
    background: -webkit-linear-gradient(top, #e0e0e0, #d1d1d1);
    /* Firefox 3.6+ */
    
    background: -moz-linear-gradient(top, #e0e0e0, #d1d1d1);
    border: solid 1px #d1d1d1;
    cursor: default !important;
}
.actionBtn.disabled label,
.actionBtn.disabled a,
.actionBtn.disabled a label,
.secondaryActionBtn.disabled,
.secondaryActionBtn.disabled a,
.secondaryActionBtn.disabled a label {

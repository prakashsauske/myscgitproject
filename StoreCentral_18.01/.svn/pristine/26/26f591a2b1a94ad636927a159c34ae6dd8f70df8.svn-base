/* Styles for Footer */
.footerWrapper {
	background-color: #d3d3d3;
	color: #494949;
	padding: 15px 20px;
	overflow: auto;
	position: absolute;
	bottom: 0;
	width: 940px;
	height: 15px; /*My change*/
	min-width: 940px;
}

.footer {
	overflow: auto;
	padding: 0px; /*My change*/
	width: 940px;
}

.footer .copyrightsInfo {
	float: left;
}

.footer .policyInfo {
	float: right;
}

.footer .policyInfo a {
	color: #494949;
}

.woolworths .footer .policyInfo a:hover ,.metro .footer .policyInfo a:hover {
	color: #619d1c;
}

.petrol .footer .policyInfo a:hover {
	color: #619d1c;
}

.countdown .footer .policyInfo a:hover {
	color: #619d1c;
}

.bws .footer .policyInfo a:hover {
	color: #d65100;
}

.bigw .footer .policyInfo a:hover {
	color: #4ba4c4;
}

.danmurphy .footer .policyInfo a:hover {
	color: #293b13;
}

.thomasdux.footer .policyInfo a:hover {
	color: #a17c54;
}

.footer .policyInfo label.divider {
	padding: 0 5px;
}
